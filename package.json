{
  "name": "starship",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 1984",
    "build": "next build",
    "lint": "eslint \"src/**/*.{ts,tsx,json}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx,json}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\" --config ./.prettierrc.json",
    "prepare": "husky install",
    "test": "jest",
    "test:report": "jest --coverage"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ],
    "src/**/*.{css,scss,sass}": [
      "npx prettier --write"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>src/mocks/fileMock.ts"
    },
    "collectCoverageFrom": [
      "src/**/*.tsx"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom",
      "./jest.setup.ts"
    ],
    "testEnvironmentOptions": {
      "customExportConditions": [
        ""
      ]
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@types/react-redux": "^7.1.30",
    "axios": "^1.5.1",
    "identity-obj-proxy": "^3.0.0",
    "next": "14.0.3",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading-skeleton": "^3.3.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "redux": "^4.2.1",
    "sass": "^1.69.4",
    "undici": "^5.27.2",
    "whatwg-fetch": "^3.6.19"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-typescript": "^7.22.5",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.14.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.2.3",
    "msw": "^2.0.4",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2",
    "@types/next-page-tester": "latest_version",
    "next-page-tester": "latest_version"
  }
}
